{
    "env": {
        "browser": true,
        "node": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@docusaurus/recommended",
        "plugin:react/recommended"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react", "@docusaurus"],
    "rules": {
        "@docusaurus/string-literal-i18n-messages": "error",
        "@docusaurus/no-untranslated-text": "warn",
        "react/prop-types": "warn"
    }
}
